{
    "name": "grafmodellezo",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^4",
        "@sap/cds-odata-v2-adapter-proxy": "^1.4.55",
        "express": "^4"
    },
    "devDependencies": {
        "sqlite3": "^5"
    },
    "scripts": {
        "start": "npx cds run"
    },
    "cds": {
        "hana": {
            "deploy-format": "hdbtable"
        },
        "build": {
            "target": "gen",
            "tasks": [
                {
                    "src": "db",
                    "for": "hana",
                    "options": {
                        "model": [
                            "db",
                            "srv"
                        ]
                    }
                },
                {
                    "src": "srv",
                    "for": "node-cf",
                    "options": {
                        "model": [
                            "db",
                            "srv"
                        ]
                    }
                }
            ]
        }
    }
}
